			for(int i = 0; i < FirstLine.length ;i++){
				System.out.println(FirstLine[i]);
				//Sector sector = new Sector(FirstLine[i]);
				SectorList.add(new Sector(FirstLine[i]));
				
				//System.out.println(temp.getName());
			}
			System.out.println(SectorList);
			while(MapData.hasNextLine()){
				String input = MapData.nextLine();
				for(int j = 0; j < SectorList.size() ;j++){
					
				}
			}
			MapData.close();
			
			
			
			
			
		}
		catch(FileNotFoundException e){
			System.out.println("File not Found");
		}

		//Use this space to initialize all sectors 
		
		
		
	}
}